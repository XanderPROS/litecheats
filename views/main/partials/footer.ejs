<div class="homeContainerRight">
  <div class=" loginContainer">
    
        
        
        <form autocomplete="off" action="/login" method="POST">
            <a href="/login"><img src="/images/litecheats.png" alt=""></a>
            <%if(error && error.length > 0){%>
                <div class="alert alert-danger" id="errorBox" role="alert">
                    <h6>There was a problem</h6>
                    <i id="errIco" class="fas fa-exclamation-triangle"></i> 
                    <p id="err"><%= error %></p>
                </div>
            <%}%>
            <%if(success && success.length > 0){%>
                <div class="alert alert-success" id="successBox" role="alert">
                    <h6 class="headAlert">Done</h6>
                    <i id="sucIco" class="fas fa-check-circle"></i>
                    <p id="suc"><%= success %></p>
                </div>
            <%}%>
            <% if((!error || error.length<=0) && (!success || success.length<=0)){%>
                <hr id="hrbelowIm">
            <% } %>
            <h4 id="heading">Sign-In</h4>
            <div class="form-group" autocomplete="off">
                <label for="email">Email</label>
                <input type="email" class="form-control form-control-sm" id="email" name="email"  required/>
            </div>
            <div class="form-group">
                <label for="password">Password</label>
                
                <input type="password" class="form-control form-control-sm" name="password" id="password" required>
            </div>
            <button type="submit" class="btn btn-warning"><span id="buttonText">Sign-In<span></button>
            <div>
            <hr id="hrbelowFo">
            <p id="signin">New to Litecheats? <a href="/register">Sign-Up <span id="signIco"><i class="fas fa-caret-right"></i></span></a></p>
            </div>
        </form>

</div>
  <div class="homeContainerRight_searchWidget">
    
    <form class="hSearch" action="/dashboard" method="GET">
      <div class="row vx">
        <div class=" col-12 input-group input-group mb-3 mt-3 d-flex justify-content-center">
          <input type="text" autocomplete="off" class="form-control" onkeyup="showResults(this.value)" name="search" placeholder="Search posts..." aria-describedby="button-addon2">
          <div class="input-group-append">
            <button id="searchButton" type="submit" class="btn btn-danger" type="button" id="button-addon2"><i class="fas fa-search"></i></button>
          </div>
          <div style="position: relative;width:100%;top:-1px;">
              <div class="border" id="search-results">
              </div>
            </div>
        </div>
      </div>
    </form>
  </div>
